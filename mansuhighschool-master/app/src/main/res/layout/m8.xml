    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".yooyoungbin">

    <Button
        android:id="@+id/앞"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:res="@drawable/앞"/>

    <Button
