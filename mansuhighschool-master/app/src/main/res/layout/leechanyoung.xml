<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".leechanyoung">

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="402dp"
        android:layout_height="161dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="#00000000"
        android:contentDescription="@string/error2"
        android:scaleType="centerInside"
        android:src="@drawable/button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button1_lcy"
        android:layout_width="290dp"
        android:layout_height="80dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="56dp"
        android:text="공지사항"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@id/button5"
        app:layout_constraintTop_toBottomOf="@id/button5" />

    <Button
        android:id="@+id/button2_lcy"
        android:layout_width="290dp"
        android:layout_height="80dp"
        android:layout_marginTop="50dp"
        android:text="가정통신문"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@id/button1_lcy"
        app:layout_constraintTop_toBottomOf="@id/button1_lcy" />

    <Button
        android:id="@+id/button3_lcy"
        android:layout_width="290dp"
        android:layout_height="80dp"
        android:layout_marginTop="50dp"
        android:text="학사일정"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@id/button2_lcy"
        app:layout_constraintTop_toBottomOf="@id/button2_lcy" />

</androidx.constraintlayout.widget.ConstraintLayout>